nextflow_process {

    name "Test Process FINAL"
    script "main.nf"
    process "FINAL"

    test("Should combine predictions") {

        when {
            process {
                """
                input[0] =[
                    file("${projectDir}/tests/data/108AT_test_prediction.txt"),
                    file("${projectDir}/tests/data/SVP_test_prediction.txt")]
                """
            }
        }

        then {
            assert process.success
            assert snapshot(process.out).match()
        }
    }
}